@article{cook2023,
  title = {Economic Impact of Polyphagous Shot Hole Borer {{Euwallacea}} Fornicatus ({{Coleoptera}}: {{Curculionidae}}: {{Scolytinae}}) in {{Western Australia}}},
  shorttitle = {Economic Impact of Polyphagous Shot Hole Borer {{Euwallacea}} Fornicatus ({{Coleoptera}}},
