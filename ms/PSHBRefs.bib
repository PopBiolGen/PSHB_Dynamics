@article{walgama2007,
  title = {Temperature-Dependent Development of {{Xyleborus}} Fornicatus ({{Coleoptera}}: {{Scolytidae}}), the Shot-Hole Borer of Tea in {{Sri Lanka}}: {{Implications}} for Distribution and Abundance},
  shorttitle = {Temperature-Dependent Development of {{Xyleborus}} Fornicatus ({{Coleoptera}}},
  author = {Walgama, Ravindra Sampath and Zalucki, Myron Phillip},
  year = {2007},
  journal = {Insect Science},
  volume = {14},
  number = {4},
  pages = {301--308},
  issn = {1744-7917},
  doi = {10.1111/j.1744-7917.2007.00156.x},
  urldate = {2023-11-20},
  abstract = {The effect of temperature on the rate of development of Xyleborus fornicatus (Eichh.) was determined by rearing individuals under a range of constant temperatures (15--32{$^\circ$}C). Rates of development changed in a linear fashion over a wide range of temperatures. Estimates of lower development thresholds were obtained for eggs (15.7{\textpm}0.5{$^\circ$}C), larvae (15.8{\textpm}0.8{$^\circ$}C) and pupae (14.3{\textpm}1.4{$^\circ$}C) and the degree days (DD) for development were 70{\textpm}4.4, 95{\textpm}8.5 and 72{\textpm}5.1 DD, respectively. Optimum temperature for development was around 30{$^\circ$}C for all stages. Temperature fluctuation in cooler High Country areas (above 1400 m) with a mean temperature around 15{$^\circ$}C seems to be critical for the development of the pest, which may be responsible for the near absence of pest in those areas. Temperature fluctuations (18--30{$^\circ$}C) in the Mid Country region (600--1200 m) favor the development of the pest compared to development under constant conditions. The altitudinal distribution of the shot-hole borer across tea growing areas in Sri Lanka is, therefore, mainly governed by temperature.},
  langid = {english},
  keywords = {damage levels,degree-days,developmental thresholds,tea growing regions},
  file = {/Users/293003c/Zotero/storage/9PN5M56G/Walgama and Zalucki - 2007 - Temperature-dependent development of Xyleborus for.pdf;/Users/293003c/Zotero/storage/2NB37H7I/j.1744-7917.2007.00156.html}
}

